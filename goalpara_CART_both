/***** End of imports. If edited, may not auto-convert in the playground. *****/
var my_roi = roi; 
// Load Landsat data
var image14 = ee.ImageCollection("LANDSAT/LC08/C01/T1_SR")
              .filterDate('2014-01-01', '2014-12-31')
              .filterBounds(my_roi)
              .sort('CLOUD_COVER')
              .first();
              
var image21 = ee.ImageCollection("LANDSAT/LC08/C01/T1_SR")
              .filterDate('2021-01-01', '2021-12-31')
              .filterBounds(my_roi)
              .sort('CLOUD_COVER')
              .first();
              
var visualization = {
                      bands: ['B4', 'B3', 'B2'],
                      min: 0.0,
                      max: 3000,
                      gamma:1.4
                    };

Map.addLayer(image14, visualization, 'Landsat_2014_Goalpara');
Map.addLayer(image21, visualization, 'Landsat_2021_Goalpara');

Map.centerObject(my_roi, 7);

// //Create training data

var label = "Class";

var bands = ['B1', 'B2', 'B3', 'B4', 'B5', 'B6', 'B7'];

var input14 = image14.select(bands);

var input21 = image21.select(bands);

var training14 = Forest.merge(NonForest);

print("training14", training14);

var training21 = ForestNew.merge(NonForestNew);

print("training21", training21);

// Overlay points on the img for training

var trainImage14 = input14.sampleRegions({
                    collection: training14,
                    properties: [label],
                    scale: 30
                });
                
var trainImage21 = input21.sampleRegions({
                    collection: training21,
                    properties: [label],
                    scale: 30
                });
                 
print("trainImage14", trainImage14);
print("trainImage21", trainImage21);

var trainingData14 = trainImage14.randomColumn();
var trainSet14 = trainingData14.filter(ee.Filter.lessThan('random', 0.8));
var testSet14 = trainingData14.filter(ee.Filter.greaterThanOrEquals('random', 0.8));

var trainingData21 = trainImage21.randomColumn();
var trainSet21 = trainingData21.filter(ee.Filter.lessThan('random', 0.8));
var testSet21 = trainingData21.filter(ee.Filter.greaterThanOrEquals('random', 0.8));

// Classify now : CART

var classifier14 = ee.Classifier.smileCart().train(trainSet14, label, bands); //model
var classifier21 = ee.Classifier.smileCart().train(trainSet21, label, bands); //model
// // Train a random forest classifier with default parameters.
// var classifier_rf = ee.Classifier.smileRandomForest({numberOfTrees: 10}).train({
//   features: trainSet_rf,
//   classProperty: 'Class',
//   inputProperties: bands
// });

var classified14 = input14.classify(classifier14);
print("CART classfication 2014 info ", classified14.getInfo());

var classified21 = input21.classify(classifier21);
print("CART classfication 2021 info ", classified21.getInfo());

// var classified_rf = input.classify(classifier_rf);
// print("RF classfication info ",classified_rf.getInfo());

// //Classification Pallette

var lcPallette = [
  '#005a32', //Forest
  '#0c2c84' ///Non Forest
  ]
  
Map.addLayer(classified14, {palette: lcPallette, min:0, max:1}, 'classification14');

Map.addLayer(classified21, {palette: lcPallette, min:0, max:1}, 'classification21');

// // find accuracy now

var confusionMatrix14 = ee.ConfusionMatrix(testSet14.classify(classifier14)
                        .errorMatrix({
                          actual: 'Class',
                          predicted: 'classification'
                        }));
                        
print("Confusion Matrix 14 : ", confusionMatrix14);

print("Accuracy 14 : ", confusionMatrix14.accuracy());

var confusionMatrix21 = ee.ConfusionMatrix(testSet21.classify(classifier21)
                        .errorMatrix({
                          actual: 'Class',
                          predicted: 'classification'
                        }));
                        
print("Confusion Matrix 21 : ", confusionMatrix21);

print("Accuracy 21 : ", confusionMatrix21.accuracy());
// Export Satellite image to google drive

var sat_14= image14.visualize (visualization);
var sat_21= image21.visualize (visualization);

Export.image.toDrive({
  image: sat_14,
  description: 'Landsat_Goalpara_2014',
  scale: 30,
  region: image14.geometry(),
  maxPixels: 1e13
});

Export.image.toDrive({
  image: sat_21,
  description: 'Landsat_Goalpara_2021',
  scale: 30,
  region: image14.geometry(),
  maxPixels: 1e13
});


// //Export classified image to google drive

// var image_RGB = image.visualize({'bands': ["B4", "B3", "B2"], max:0.5});   
var classified_RGB_14= classified14.visualize ({
  'min':0,
  'max':1,
  'palette': lcPallette
}); 

print(classified_RGB_14.getThumbURL({region:roi, scale:200, min:0, max:3000, format:'jpg'})); 

var classified_RGB_21= classified21.visualize ({
  'min':0,
  'max':1,
  'palette': lcPallette
}); 

print(classified_RGB_21.getThumbURL({region:roi, scale:200, min:0, max:3000, format:'jpg'})); 

Export.image.toDrive({
  image: classified_RGB_14,
  description: 'Landsat_CART_Goalpara_2014',
  scale: 30,
  region: image14.geometry(),
  maxPixels: 1e13
});


Export.image.toDrive({
  image: classified_RGB_21,
  description: 'Landsat_CART_Goalpara_2021',
  scale: 30,
  region: image21.geometry(),
  maxPixels: 1e13
});

